#first question
number=int(input())
import math
if math.sqrt(number)==2:
  print("True")

#second question
number=int(input())
import math
if math.sqrt(number)==3:
  print("True")

#third question
number=int(input())
import math
if math.sqrt(number)==4:
  print("True")

#fourth question
number=int(input())
import math
number2=int(math.sqrt(number))
if math.sqrt(number)=number2:
  print("True")
